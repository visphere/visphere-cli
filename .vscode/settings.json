{
  "workbench.colorCustomizations": {
    "activityBar.background": "#583e15",
    "titleBar.activeBackground": "#583e15",
    "titleBar.inactiveBackground": "#583e15"
  },
  "prettier.configPath": "./.prettierrc.js",
  "editor.detectIndentation": false,
  "editor.mouseWheelZoom": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "all",
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "workbench.tree.indent": 20,
  "workbench.iconTheme": "vsc-jetbrains-icons-enhanced",
  "workbench.preferredDarkColorTheme": "One Dark Pro Monokai Darker",
  "files.insertFinalNewline": true,
  "psi-header.variables": [
    ["msph_projectName", "moonsphere"],
    ["msph_moduleName", "moonsphere-cli"]
  ],
  "psi-header.config": {
    "forceToTop": true,
    "blankLinesAfter": 1
  },
  "psi-header.changes-tracking": {
    "isActive": true,
    "autoHeader": "autoSave",
    "include": ["javascript"],
    "enforceHeader": true,
    "replace": ["File name", "Last modified"]
  },
  "psi-header.lang-config": [
    {
      "language": "javascript",
      "begin": "/*",
      "lineLength": 64,
      "beforeHeader": ["'use strict';"]
    },
    {
      "language": "*",
      "begin": "/*",
      "lineLength": 64
    }
  ],
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "Copyright (c) <<year>> by MoonSphere Systems",
        "Originally developed by Mi≈Çosz Gilga <https://miloszgilga.pl>"
      ]
    }
  ]
}
